generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  price       Decimal
  description String
  imageUrl    String
  createdAt   DateTime @default(now()) @map("created_at")

  @@map("products")
}
